FILE_UPLOAD_MAX_SIZE=10GB

WEBSOCKET_URL=

SITE_MULTIPLE_DATA_DELETE_REQUEST_LIMIT=10

ASSET_REGISTERABLE_NUM=5
ASSET_AVAILABLE_TIME=86400000
ASSET_SYSTEM_USER_ID=SYSTEM(patchAssets)
# Upper limit for CesiumIon asset conversion in asset batches (default 10)
ASSET_BATCH_CONVERT_LIMIT=

CESIUM_ION_API_URL=
CESIUM_ION_ACCESS_TOKEN=

VIEW_API_DOCUMENT=true

LOCAL_TOKEN=
